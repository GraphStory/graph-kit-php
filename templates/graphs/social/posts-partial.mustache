<tr id="tr_{{ getContentId }}">
    <td>
        <a href="{{ getUrl }}" id="url_{{ getContentId }}" target="_blank">{{ getTitle }}</a><br>
        tags: {{ getTagstr }} :: Posted by {{ getOwner.getUsername }} at <a href="{{ getPostUrl }}{{ getContentId }}">{{ getTimestamp }}</a>
    </td>
    <td>
        {{# owner }}
            <button type="button" class="editcontent btn btn-primary btn-xs" id="edit_{{ getContentId }}">Edit</button>&nbsp;
            <button type="button" class="deletecontent btn btn-danger btn-xs" id="delete_{{ getContentId }}">Delete</button>
        {{/ owner }}<br>
    </td>
</tr>
